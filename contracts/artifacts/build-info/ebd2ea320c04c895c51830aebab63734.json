		"sources": {
			"contracts/Hometask.sol": {
				"content": "// SPDX-License-Identifier: MIT\r\n\r\npragma solidity ^0.8.0;\r\n\r\ncontract DormitoryQuest {\r\n    uint constant DURATION = 3 days;\r\n\r\n    struct Student {\r\n        string fullName;\r\n        bool sheetsDone;\r\n        bool roomDone;\r\n        bool booksDone;\r\n        uint regDate;\r\n    }\r\n\r\n    event SummonsToArmyGiven(string name);\r\n    event StudentEvicted(string name, uint date);\r\n\r\n    function debtsAreDone(Student storage _student) private {\r\n        if (_student.sheetsDone == false)\r\n        {\r\n            emit SummonsToArmyGiven(_student.fullName);\r\n        }\r\n\r\n        if (_student.roomDone == false)\r\n        {\r\n            emit SummonsToArmyGiven(_student.fullName);\r\n        }\r\n\r\n        if (_student.booksDone == false)\r\n        {\r\n            emit SummonsToArmyGiven(_student.fullName);\r\n        }\r\n\r\n        if (_student.regDate + DURATION > block.timestamp)\r\n        {\r\n            emit SummonsToArmyGiven(_student.fullName);\r\n        }\r\n        \r\n        emit StudentEvicted(_student.fullName, block.timestamp);\r\n    }\r\n}"
			}
		},
